{
   "server": {
    "host": "127.0.0.1",
    "port": 8080
  },
  "control": {
    "impl_module": "RIPSimulink",
    "info": {
      "name": "my_cs_quarc",
      "description": "A simple MATLAB-Simulink example",
      "authors": "L. de la Torre",
      "keywords": "Raspberry PI, RIP",
      "model": "examples/quarc_sine_scope.slx",
      "readables": [{
        "name":"SimulationTime",
        "description":"Server time in seconds",
        "type":"float",
        "min":"0",
        "max":"Inf",
        "precision":"0"
      },
      {
        "name":"signal",
        "description":"Output signal",
        "type":"float",
        "min":"0",
        "max":"Inf",
        "precision":"0"
      }],
      "writables": [{
        "name":"amplitude",
        "description":"Input signal",
        "type":"float",
        "min":"0",
        "max":"Inf",
        "precision":"0"
      }]
    }
  }
}
